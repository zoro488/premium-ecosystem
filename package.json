{
  "name": "premium-ecosystem",
  "version": "3.0.0",
  "description": "Ecosistema Premium con 5 aplicaciones empresariales",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "rimraf dist node_modules node_modules/.vite",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:preview": "npm run build && firebase hosting:channel:deploy preview",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "quick-deploy": "pwsh -File deploy-quick.ps1",
    "auto-fix": "pwsh -File auto-fix.ps1",
    "diagnose": "node scripts/diagnose-tests.cjs",
    "fix:connection": "pkill -f firebase; firebase emulators:start --only firestore",
    "fix:types": "npm install -D @types/node@latest @types/react@latest",
    "fix:cache": "npm cache clean --force && rm -rf node_modules && npm install",
    "fix:all": "npm run fix:cache && npm run fix:types",
    "prepare": "husky"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.2",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/react": "^10.21.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "8.20",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1",
    "date-fns": "^3.6.0",
    "firebase": "^12.4.0",
    "framer-motion": "11.11",
    "idb-keyval": "^6.2.2",
    "lucide-react": "^0.441.0",
    "million": "3.0",
    "react": "^18.3.1",
    "react-confetti": "^6.4.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.30.1",
    "react-virtualized-auto-sizer": "1.0",
    "react-window": "1.8",
    "recharts": "^2.15.4",
    "socket.io-client": "4.7",
    "tailwind-merge": "^2.6.0",
    "use-debounce": "^10.0.6",
    "xlsx": "^0.18.5",
    "y-websocket": "1.5",
    "yjs": "13.6",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "vite": "^5.4.21",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
