                              Online                      <th className="text-left py-3 px-4 font-semibold text-gray-700">

                            </div>                        Observaciones

                          </div>                      </th>

                        </div>                    </tr>

                      </motion.div>                  </thead>

                    );                  <tbody>

                  })}                    {filteredGastos.map((gasto, index) => (

                </div>                      <motion.tr

              </div>                        key={`${gasto.fecha}-${index}`}

