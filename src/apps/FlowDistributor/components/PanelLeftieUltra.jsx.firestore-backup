                  whileHover={{ scale: 1.1 }}      {/* Controles */}

                  onClick={() => setModoSeguro(!modoSeguro)}      <motion.div

                >        className="bg-white/80 backdrop-blur-lg rounded-2xl p-6 mb-8 shadow-xl border border-white/20"

