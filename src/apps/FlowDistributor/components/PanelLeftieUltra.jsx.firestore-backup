                        animate={{ opacity: 1, y: 0 }}

          {/* Placeholder para otras tablas */}                        transition={{ delay: index * 0.05 }}

          {activeTable !== 'cuentas' && (                      >

            <motion.div                        <td className="py-3 px-4 text-gray-600">{gasto.fecha}</td>

              key={activeTable}                        <td className="py-3 px-4 font-medium text-gray-800">{gasto.origen}</td>

