{
  "extends": ["./.eslintrc.json"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "error",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}
